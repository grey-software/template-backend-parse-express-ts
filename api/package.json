{
  "name": "parse-express-backend-template",
  "version": "0.0.1",
  "description": " A template for an app backend that uses Node, Parse & Express and is written in Typescript. ",
  "repository": "https://github.com/grey-software/parse-express-backend-template.git",
  "author": "Grey Khan",
  "license": "MIT",
  "scripts": {
    "start": "$(yarn bin)/pm2 start index.ts",
    "dev": "$(yarn bin)/pm2 start index.ts --watch --no-daemon",
    "postinstall": "$(yarn bin)/pm2 install typescript",
    "stop": "$(yarn bin)/pm2 stop index"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "parse": "^2.18.0",
    "parse-dashboard": "^2.1.0",
    "parse-server": "^4.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.10",
    "@types/parse": "^2.18.1",
    "pm2": "^4.5.0",
    "ts-node": "^9.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  }
}
